{
  "name": "@greenlabs/formula-components",
  "version": "0.0.1-alpha.11",
  "repository": {
    "type": "git",
    "url": "git@github.com:green-labs/formula-design-system.git",
    "directory": "packages/components"
  },
  "author": "Greenlabs Dev <developer@greenlabs.co.kr>",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/ts/index.d.ts",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "@greenlabs/formula-design-token": "0.0.1-alpha.6"
  },
  "files": [
    "build/",
    "!build/ts/**/*.tsbuildinfo"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.20",
    "@vanilla-extract/css": "^1.9.0",
    "@vanilla-extract/rollup-plugin": "^1.1.0",
    "@vanilla-extract/webpack-plugin": "^2.1.12",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "rollup": "^2.79.0",
    "rollup-plugin-node-externals": "^5.0.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "deploy:storybook": "storybook-to-ghpages",
    "build:js": "rollup -c",
    "build:types": "tsc -b",
    "build": "yarn clean && yarn build:js && yarn build:types",
    "clean": "rm -rf ./build",
    "start": "concurrently -g -n re,storybook \"yarn re:watch\" \"yarn storybook\"",
    "prepublish": "yarn build",
    "build:ts-icons": "cd src/Icon && yarn dlx @svgr/cli svgs --no-index && node ./templates/export-generator.js",
    "build:res-icons": "cd src/Icon && node ./templates/rescript-binder.js",
    "svgr": "yarn build:ts-icons && yarn build:res-icons"
  }
}
